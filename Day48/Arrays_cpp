#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int nums;
    cin>>nums;
    int a[nums];
    for(int i=0; i<nums; i++){
        cin>>a[i];
    }
    for(int i=nums-1; i>=0; i--){
        cout<<a[i]<<" ";
    }
    return 0;
}
