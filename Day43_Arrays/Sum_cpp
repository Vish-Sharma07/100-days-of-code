#include<iostream>
using namespace std;
int main()
{
    int arr[] = {12,4,6,34,2,9};
    int sum = 0;
    cout<<"Given array is: "<<endl;
    for(int i =0; i<=5; i++){
        cout<<arr[i]<<" "<<endl;
    }
    cout<<"Sum of given array is:"<<endl;
    for(int i =0; i<=5; i++){
        sum = sum + arr[i];
    }
    cout<<sum<<endl;
}
