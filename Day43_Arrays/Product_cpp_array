#include<iostream>
using namespace std;
int main()
{
    int arr[] = {12,4,6,34,2,9};
    int prod = 1;
    cout<<"Given array is: "<<endl;
    for(int i =0; i<=5; i++){
        cout<<arr[i]<<" "<<endl;
    }
    cout<<"Product of given array is:"<<endl;
    for(int i =0; i<=5; i++){
        prod = prod * arr[i];
    }
    cout<<prod<<endl;
}
