#include<iostream>
using namespace std;
int main(){
    int n;
    cout<<"Enter no of elements: "<<endl;;
    cin>>n;
    for(int i=1; i<=n; i++){
        cout<<i<<" "<<endl;;
    }
    cout<<"Square of elements from 1 to "<<n << " is:"<<endl;
    for(int i=1;i<=n; i++){
        int square = i*i;
        cout<<square<<" "<<endl;
    }
}
